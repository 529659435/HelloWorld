<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ym.mybatisdemo.dao.UserDao">

    <select id="getAllUser" resultType="com.ym.mybatisdemo.bean.User">
        SELECT * FROM user;
    </select>

    <insert id="insert" parameterType="com.ym.mybatisdemo.bean.User" useGeneratedKeys="true" keyProperty="userId">
        insert into
        user(id,userName,age, sex)
        values
        (#{id},#{userName},#{age},#{sex})
    </insert>
</mapper>